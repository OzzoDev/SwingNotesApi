    "repository": {
        "type": "git",
        "url": "git+https://github.com/OzzoDev/SwingNotesApi.git"
    },
    "author": "OzzoDev",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/OzzoDev/SwingNotesApi/issues"
    },
    "homepage": "https://github.com/OzzoDev/SwingNotesApi#readme",
    "dependencies": {
        "bcryptjs": "^3.0.2",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.16.0",
        "swagger-ui-express": "^5.0.1",
        "zod": "^3.25.46"
    },
    "devDependencies": {
        "@babel/core": "^7.27.4",
        "@babel/preset-env": "^7.27.2",
        "@types/jest": "^29.5.14",
        "cross-env": "^7.0.3",
